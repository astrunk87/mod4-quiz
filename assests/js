var timeEl = document.querySelector(".time");



function timer () {
    var timerInterval = setInterval(function(){
        secondsLeft--;
        timeEl.textContent = secondsLeft + "tell game over";

        if(secondsLeft === 0) {
            clearInterval(timerInterval);

            sendMessage();
        }
    }, 1000);

}

function sendMessage() {
    timeEl.textContent = "GAME OVER"
}